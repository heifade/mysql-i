{
  "name": "mysql-i",
  "version": "3.0.11",
  "description": "mysql interface, base on mysql",
  "main": "./es/index.js",
  "module": "./es/index.js",
  "files": [
    "es"
  ],
  "dependencies": {
    "mysql": "^2.15.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.5",
    "@types/mocha": "^2.2.44",
    "@types/mysql": "^2.15.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "source-map-support": "^0.5.0",
    "ts-node": "^3.3.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.1"
  },
  "scripts": {
    "removeDist": "node ./tools/deleteOutPath.js",
    "tsBuild": "tsc -p tsconfig.json",
    "docs": "typedoc --out docs src --module commonjs --hideGenerator && node ./tools/replaceDocFileUnderline.js",
    "build": "npm run removeDist && npm run tsBuild && npm run docs",
    "test": "nyc mocha -t 5000",
    "test-nyc": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "keywords": [
    "mysql",
    "mysql-i",
    "promise"
  ],
  "author": "heifade",
  "license": "MIT"
}
